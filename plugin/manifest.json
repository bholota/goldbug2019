{
    "name": "Hackathon",
    "version": "1.0.0",
    "description": "Jez soft",
    "icons": {
        "16": "img/icon_16x16.png",
        "48": "img/icon_48x48.png",
        "128": "img/icon_128x128.png"
    },
    "default_locale": "pl",
    "homepage_url": "http://www.ferfecki.top",
    "browser_action": {
        "default_icon": {
            "19": "img/icon_19x19.png",
            "38": "img/icon_38x38.png"
        },
        "default_title": "Bezpieczny Je≈º",
        "default_popup": "popup.html"
    },
    "devtools_page": "/devtools/devtools-page.html",
    "background": {
      "scripts": [
        "/lib/jquery-3.3.1.min.js",
        "/lib/bootstrap/bootstrap.min.js",
        "/lib/bootstrap-table/bootstrap-table.min.js",
        "/lib/apexcharts/apexcharts.min.js",
        "/lib/springy/springyui.js",
        "/lib/springy/springy.js",
        "/lib/object-watch.js",
        "/js/cookie_helpers.js",
        "/js/utils.js",
        "/js/data.js",
        "/js/background.js"
      ]
    },
    "options_page": "options_main_page.html",
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E",
    "cookies",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
